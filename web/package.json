{
  "name": "web",
  "scripts": {
    "build-client": "browserify --entry client.js > public/bundle.js",
    "start": "babel-node server.js",
    "dev": "npm-run-all --parallel  watch:*",
    "watch:client": "watchify client.js --debug -o public/bundle.js -v",
    "watch:server": "nodemon --exec babel-node server.js"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "presets": [
      "react"
    ]
  },
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-react": "6.24.1",
    "babelify": "7.3.0",
    "bluebird": "3.5.0",
    "browserify": "14.4.0",
    "c3": "0.4.18",
    "connect-redis": "3.3.0",
    "express": "4.15.4",
    "express-session": "1.15.5",
    "moment": "2.18.1",
    "moment-timezone": "0.5.13",
    "passport": "0.4.0",
    "passport-discord": "0.1.2",
    "pg-promise": "6.8.0",
    "react": "15.6.1",
    "react-dom": "15.6.1"
  },
  "devDependencies": {
    "nodemon": "1.12.1",
    "npm-run-all": "4.1.1",
    "watchify": "3.9.0"
  }
}
