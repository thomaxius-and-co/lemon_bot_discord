{
  "name": "web",
  "scripts": {
    "build-client": "browserify --entry client.js > public/bundle.js",
    "start": "babel-node server.js",
    "dev": "npm-run-all --parallel  watch:*",
    "watch:client": "watchify client.js --debug -o public/bundle.js -v",
    "watch:server": "nodemon --exec babel-node server.js"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "presets": [
      "react"
    ]
  },
  "dependencies": {
    "@sentry/browser": "^6.3.3",
    "axios": "^0.21.1",
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babelify": "8.0.0",
    "bluebird": "3.5.1",
    "browserify": "16.2.2",
    "c3": "0.6.2",
    "connect-redis": "3.3.3",
    "express": "4.17.1",
    "express-session": "1.15.6",
    "moment": "2.29.1",
    "moment-timezone": "0.5.33",
    "npm-check": "^5.9.2",
    "passport": "0.4.1",
    "passport-discord": "0.1.4",
    "pg-promise": "10.10.1",
    "react": "16.4.1",
    "react-dom": "^16.13.1",
    "request": "2.88.2",
    "request-promise": "4.2.2",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "nodemon": "2.0.7",
    "npm-run-all": "4.1.5",
    "watchify": "^3.11.1"
  }
}
